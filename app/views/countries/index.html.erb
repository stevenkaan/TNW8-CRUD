<div class="container content">
	<div class="col s8 n8 l8 xl8">
		<h2>Landen overzicht</h2>
		<table>
		  <tr>
		    <th>Land</th>
		  </tr>

		  <% @countries.each do |country| %>
		    <tr>
		      <td data-href='<%= country_path(country) %>'><%= link_to country.country_name, country_path(country)%></td>
		    </tr>
		  <% end %>
		</table>
<br>
		<%= render 'countries/form' %>
<br>

	</div>
<div class="col xl3 l3 m3 s3">
</div>
<div class="clear"></div>
</div>